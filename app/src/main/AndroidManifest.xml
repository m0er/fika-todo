<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="todo.fika.fikatodo">

    <application
        android:name=".FikaTodoApplication_"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".FikaLauncherActivity_"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_fika_launcher"
            android:theme="@style/FullscreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".today.FikaTodayActivity_"
            android:label="@string/title_activity_week"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>
